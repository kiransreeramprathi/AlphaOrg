<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AADHAR_number</fullName>
    <active>true</active>
    <errorConditionFormula>AND(len(Aadhar_Number__c )&lt;12,NOT(REGEX(Aadhar_Number__c ,&quot;^[0-9]&quot;)))</errorConditionFormula>
    <errorDisplayField>Aadhar_Number__c</errorDisplayField>
    <errorMessage>AADHAR number should be 12 digit</errorMessage>
</ValidationRule>
